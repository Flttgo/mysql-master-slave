#
# PHP source directory
#
SOURCE_DIR=./www

#
# Runtime data directory
#
DATA_DIR=./data

#
# Container Timezone
#
TZ=Asia/Shanghai

#
# Container package fetch url
#
# Can be empty, followings or others:
# mirrors.163.com
# mirrors.aliyun.com
# mirrors.ustc.edu.cn
# dl-cdn.alpinelinux.org
#
CONTAINER_PACKAGE_URL=mirrors.ustc.edu.cn

#
#ALPINE
#
ALPINE_VERSION=3.12

# 主库
# MySQL8-master
#
MYSQL_VERSION=8.0.13
MYSQL_HOST_PORT=33308
MYSQL_ROOT_PASSWORD=123456
MYSQL_CONF_FILE=./services/mysql/master/my.cnf
MYSQL_DATA_DIR=./data/mysql/master



# 从库
# MySQL8-slave
#
MYSQL_SLAVE_VERSION=8.0.13
MYSQL_SLAVE_HOST_PORT=33307
MYSQL_SLAVE_ROOT_PASSWORD=123456
MYSQL_SLAVE_CONF_FILE=./services/mysql/slave/my.cnf
MYSQL_SLAVE_DATA_DIR=./data/mysql/slave



